<?xml version="1.0" encoding="UTF-8"?>
<manifest>
 <!-- build -->
 <remove-project name="AOSPA/android_build" />
 <project path="build/make" name="amdev01/android_buildAOSPA" remote="github" revision="sapphire" >
   <copyfile src="core/root.mk" dest="Makefile" />
   <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
   <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
   <linkfile src="core" dest="build/core" />
   <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
   <linkfile src="target" dest="build/target" />
   <linkfile src="tools" dest="build/tools" />
 </project>
 <!-- vendor -->
 <remove-project name="AOSPA/android_vendor_aospa" />
 <project path="vendor/aospa" name="amdev01/android_vendor_aospa" remote="github" revision="sapphire" >
  <linkfile src="build.sh" dest="rom-build.sh" />
 </project>
 <!-- kernel scripts -->
 <remove-project name="AOSPA/android_kernel_build" />
 <project path="device/qcom/kernelscripts" name="amdev01/android_kernel_build" remote="github" revision="sapphire" >
   <linkfile dest="vendor/qcom/build/tasks/kernel_definitions.mk" src="kernel_definitions.mk"/>
 </project>
</manifest>
